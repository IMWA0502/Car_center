# Copy this file to .env and fill in the values

# AutoTrader Sandbox credentials
AUTOTRADER_KEY=
AUTOTRADER_SECRET=
AUTOTRADER_SANDBOX_ID=

# Use mock data to unblock frontend while sandbox endpoints are pending
# true | false
MOCK_AUTOTRADER=false

# Optionally pin the working endpoint once confirmed by AutoTrader
# Example: https://api-sandbox.autotrader.co.uk/adverts/search?advertiser_id={advertiserId}
AUTOTRADER_VEHICLES_ENDPOINT=

# Optional paging
AUTOTRADER_PAGE_SIZE=50

# Server port (default 3000)
# PORT=3000
